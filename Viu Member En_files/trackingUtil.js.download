function event_push(
    screenName,
    category,
    action,
    label,
    screen_metadata,
    callback
)
{
    var tracker = tracking.getTracker(
        screenName,
        window.tracking.TrackingUtils.getUserID()
    );
    var event = new window.tracking.Event();
    event.category = category;
    event.action = action;
    event.label = label;
    event.interactive = true;
    var variables = event.customVariables;

    if(screen_metadata != null){
        variables.addDimension(window.tracking.dimension.SCREEN_METADATA, screen_metadata);
    }
    
    // window.tracking.TrackingUtils.addDimension(window.tracking.globalDimension.USER_LEVEL, window.tracking.TrackingUtils.getDimension(window.tracking.globalDimension.USER_LEVEL));
    
    tracker.pushEvent(event, callback);

}
facade.event_push=event_push


function screen_push(
    screen_name,
    product_id,
    episode_number,
    series_id,
    category_id,
    category_name,
    screen_metadata,
    entry_point,
    callback
)
{
try{
    product_id = typeof product_id !== 'undefined' ? product_id : null;
    episode_number = typeof episode_number !== 'undefined' ? episode_number : null;
    series_id = typeof series_id !== 'undefined' ? series_id : null;
    category_id = typeof category_id !== 'undefined' ? category_id : null;
    category_name = typeof category_name !== 'undefined' ? category_name : null;
    screen_metadata = typeof screen_metadata !== 'undefined' ? screen_metadata : null;
    entry_point = typeof entry_point !== 'undefined' ? entry_point : null;

    var screenName = screen_name;
    var tracker = window.tracking.getTracker(screenName, window.tracking.TrackingUtils.getUserID());
    var pageView = new window.tracking.PageView();
    var variables = pageView.customVariables;

    if(product_id != null){
        variables.addDimension(window.tracking.dimension.EPISODE_ID, product_id);
    }
    if(episode_number != null){
        variables.addDimension(window.tracking.dimension.EPISODE_NUMBER, episode_number);
    }
    if(series_id != null){
        variables.addDimension(window.tracking.dimension.SERIES_ID, series_id);
    }
    if(category_name != null){
        variables.addDimension(window.tracking.dimension.CATEGORY_SECTION_NAME, category_name);
    }
    if(category_id != null){
        variables.addDimension(window.tracking.dimension.CATEGORY_SECTION_ID, category_id);
    }
    if(screen_metadata != null){
        variables.addDimension(window.tracking.dimension.SCREEN_METADATA, screen_metadata);
    }
    if(entry_point != null){
        variables.addDimension(window.tracking.dimension.ENTRY_POINT, entry_point);
    }
    
    tracker.pushPage(pageView,callback);

    window.tracking.TrackingUtils.addDimension(window.tracking.globalDimension.SCREEN_NAME, screen_name);
    window.tracking.TrackingUtils.refreshScreenSession(false);

}
catch (ex) {
    console.error(ex)
}

}
facade.screen_push=screen_push


